{
  "name": "tellihealth-rpm-api-spec",
  "version": "1.0.0",
  "description": "TelliHealth OpenAPI for RPM API",
  "repository": {
    "type": "git",
    "url": "https://github.com/tellihealth/tellihealth-rpm-api-spec.git"
  },
  "lint-staged": {
    "*.js": "prettier",
    "*.ts": "eslint"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "useEmoji": true
    }
  },
  "engines": {
    "node": "^20"
  },
  "keywords": [
    "OpenAPI",
    "API",
    "RPM API"
  ],
  "author": "Simun Romic <sromic@evelyn.one>",
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@commitlint/cz-commitlint": "19.8.1",
    "@redocly/cli": "2.2.2",
    "commitizen": "4.3.1",
    "cz-git": "1.12.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.3",
    "react-dom": "19.1.1"
  },
  "private": "false",
  "scripts": {
    "build:doc": "redocly build-docs openapi.yaml -o=index.html",
    "lint": "redocly lint --extends recommended openapi.yaml",
    "prepare": "husky"
  }
}